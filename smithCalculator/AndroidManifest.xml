<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.calculator"
    android:versionCode="52"
    android:versionName="22" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:name="com.example.calculator.SmithCalculatorApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.calculator.SmithCalcMainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.calculator.ResultActivity" >
        </activity>
        <activity
            android:name="com.example.calculator.FactorialActivity"
            android:label="@string/title_activity_factorial" >
        </activity>
        <activity
            android:name="com.example.calculator.FactorialBindingActivity"
            android:label="factorial binding activity" >
        </activity>

        <service
            android:name="FactorialService"
            android:label="FactorialService" >
        </service>
    </application>

</manifest>